{
    "manifest_version": 3,
    "name": "IPUT Portal+",
    "description": "IPUTポータルサイトの成績照会を使いやすくします",
    "version": "1.0.0",
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "content_scripts": [
      {
        "js": [
          "./js/main.js"
        ],
        "css": [
          "./css/style.css"
        ],
        "matches": [
          "https://portal.iput.ac.jp/*"
        ]
      }
    ]
  }